Sub Main()
    Dim i As Integer = 0
    Dim c As Integer = 0
    While i < 10
        i = i + 1
        c = c + 1
    Wend

    if c == 10 then Println("pass") else Println("failed")

    Dim c2 As Integer = 0
    // 0 + 2 + 4 + 6 + 8 + 10 = 30 
    for dim i = 0 to 10 step 2
        c2 = c2 + i
    next

    if i==0 then Println("pass") else Println("failed")

    if c2==30 then Println("pass") else Println("failed")

    Println("<terminate>")
End Sub